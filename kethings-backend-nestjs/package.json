{
  "name": "kethings-backend-nestjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
